{
  "name": "o3oThumb",
  "description": "the best o3oThumb",
  "version": "0.0.1",
  "scripts": {
    "start": "babel-node app.js",
    "watch": "nodemon --exec babel-node -- app.js",
    "debug": "babel-node-debug app.js",
    "debugWatch": "nodemon --exec babel-node-debug -- app.js",
    "build": "webpack --verbose --colors --progress --profile --display-error-details --config webpack/webpack.prod.config.js",
    "start-prod": "better-npm-run start-prod",
    "coding-start": "node ./bin/server.js",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" "
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 9100
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "development",
        "HOST": "192.168.1.105",
        "PORT": 9100
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "UV_THREADPOOL_SIZE": 100,
        "HOST": "192.168.1.105",
        "NODE_PATH": "./app"
      }
    }
  },
